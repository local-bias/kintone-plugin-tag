{
  "name": "kintone-plugin-tag",
  "version": "1.0.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip plugin && node scripts/rename-ppk.mjs",
    "build": "node scripts/marge-json.mjs prod && kbuild prod plugin --prod-root cdn && kintone-plugin-packer --out dist/plugin.zip --ppk dist/private.ppk plugin",
    "standalone": "node scripts/marge-json.mjs standalone && kbuild prod plugin --prod-root plugin && kintone-plugin-packer --out dist/plugin-standalone.zip --ppk dist/private.ppk plugin",
    "dev": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip --ppk dist/private.ppk plugin && concurrently \"yarn:dev:*\"",
    "dev:build": "kbuild dev plugin",
    "dev:upload": "node scripts/upload.mjs",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "browser": {
    "path": false
  },
  "devDependencies": {
    "@amcharts/amcharts5": "^5.5.7",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "11.11.0",
    "@kintone/dts-gen": "8.0.2",
    "@kintone/plugin-packer": "8.0.1",
    "@kintone/plugin-uploader": "9.0.1",
    "@kintone/rest-api-client": "5.0.3",
    "@kintone/webpack-plugin-kintone-plugin": "8.0.2",
    "@konomi-app/kbuild": "^3.0.0",
    "@konomi-app/kintone-utilities": "^3.8.0",
    "@konomi-app/kintone-utility-component": "^1.3.1",
    "@konomi-app/ui-react": "^1.1.1",
    "@lb-ribbit/kintone-secrets": "0.3.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.14.18",
    "@mui/material": "5.14.18",
    "@types/luxon": "3.3.4",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "concurrently": "^8.2.2",
    "dotenv": "16.3.1",
    "esbuild-loader": "4.0.2",
    "eslint": "8.54.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "immer": "10.0.3",
    "luxon": "3.4.4",
    "notistack": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "recoil": "0.7.7",
    "typescript": "5.2.2"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "endOfLine": "lf"
  }
}
