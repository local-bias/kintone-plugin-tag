{
  "name": "kintone-plugin-template",
  "version": "1.2.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "init": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip plugin && node scripts/rename-ppk.mjs",
    "build": "node scripts/marge-json.mjs prod && kbuild prod plugin --prod-root cdn && kintone-plugin-packer --out dist/plugin.zip --ppk dist/private.ppk plugin",
    "standalone": "node scripts/marge-json.mjs standalone && kbuild prod plugin --prod-root plugin && kintone-plugin-packer --out dist/plugin-standalone.zip --ppk dist/private.ppk plugin",
    "dev": "node scripts/marge-json.mjs dev && kintone-plugin-packer --out dist/plugin-dev.zip --ppk dist/private.ppk plugin && concurrently \"yarn:dev:*\"",
    "dev:build": "kbuild dev plugin",
    "dev:upload": "node scripts/upload.mjs",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "browser": {
    "path": false
  },
  "devDependencies": {
    "@amcharts/amcharts5": "^5.3.15",
    "@emotion/css": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "11.11.0",
    "@kintone/dts-gen": "6.1.33",
    "@kintone/kintone-ui-component": "^0.9.4",
    "@kintone/plugin-packer": "6.0.39",
    "@kintone/plugin-uploader": "7.1.10",
    "@kintone/rest-api-client": "3.3.15",
    "@kintone/webpack-plugin-kintone-plugin": "6.0.39",
    "@konomi-app/kbuild": "^1.5.1",
    "@konomi-app/kintone-utilities": "^2.7.0",
    "@konomi-app/kintone-utility-component": "^0.9.0",
    "@konomi-app/ui-react": "^1.1.1",
    "@lb-ribbit/kintone-secrets": "0.3.0",
    "@lb-ribbit/kintone-xapp": "0.4.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.13.4",
    "@types/luxon": "3.3.0",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "concurrently": "^8.1.0",
    "dotenv": "16.1.4",
    "esbuild-loader": "3.0.1",
    "eslint": "8.42.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "immer": "10.0.2",
    "luxon": "3.3.0",
    "notistack": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.9",
    "recoil": "0.7.7",
    "typescript": "5.1.3"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "endOfLine": "lf"
  }
}
