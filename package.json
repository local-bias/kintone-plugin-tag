{
  "name": "kintone-plugin-template",
  "version": "1.0.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.ts",
    "init": "run-s merge-manifest:prod create-ppk move-ppk",
    "bundle:dev": "webpack --config webpack.dev.js",
    "bundle:prod": "webpack --config webpack.prod.js",
    "bundle:watch": "webpack --config webpack.dev.js -w",
    "merge-manifest:dev": "ts-node scripts/marge-json.ts dev",
    "merge-manifest:prod": "ts-node scripts/marge-json.ts prod",
    "develop": "run-s merge-manifest:dev bundle:dev",
    "build": "run-s  merge-manifest:prod bundle:prod",
    "watch": "run-s merge-manifest:dev bundle:watch",
    "upload": "ts-node scripts/upload.ts",
    "start": "run-p watch upload",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "11.10.4",
    "@kintone/dts-gen": "6.1.8",
    "@kintone/plugin-packer": "6.0.11",
    "@kintone/plugin-uploader": "7.1.2",
    "@kintone/rest-api-client": "3.1.10",
    "@kintone/webpack-plugin-kintone-plugin": "6.0.11",
    "@lb-ribbit/kintone-secrets": "0.2.4",
    "@lb-ribbit/kintone-xapp": "0.3.3",
    "@mui/icons-material": "5.10.3",
    "@mui/material": "5.10.4",
    "@types/luxon": "3.0.1",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "dotenv": "16.0.2",
    "esbuild-loader": "2.20.0",
    "eslint": "8.23.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "immer": "9.0.15",
    "luxon": "3.0.3",
    "notistack": "2.0.5",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "recoil": "0.7.5",
    "terser-webpack-plugin": "5.3.6",
    "ts-node": "10.9.1",
    "typescript": "4.8.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-merge": "5.8.0"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "endOfLine": "lf"
  }
}
